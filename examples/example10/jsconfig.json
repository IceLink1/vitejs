{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~components/*": [
                "src/components/*"
            ],
            "~styles/*": [
                "src/styles/*"
            ],
            "~/*": [
                "src/*"
            ],
            "components/*": [
                "src/components/*"
            ],
            "styles/*": [
                "src/styles/*"
            ],
            "/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    }
}